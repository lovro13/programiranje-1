      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "vscode": {
          "languageId": "ocaml"
        }
      },
      "outputs": [],
      "source": [
        "type naravno =\n",
        "  | Nic\n",
        "  | Naslednik of naravno"
